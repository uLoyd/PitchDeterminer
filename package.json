{
  "name": "pitchdeterminer",
  "version": "0.5.8",
  "description": "Electron app determining pitch using web audio api",
  "main": "app.js",
  "dependencies": {
    "electron": "^11.5.0",
    "electron-remote": "^1.3.0"
  },
  "devDependencies": {
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "spectron": "^15.0.0",
    "web-audio-test-api": "^0.5.2"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "nyc npm run test",
    "check": "npx prettier --check .",
    "validate": "npm run check && npm run test",
    "format": "npx prettier --write .",
    "start": "electron .",
    "bench": "node ./test/bench/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uLoyd/PitchDeterminer.git"
  },
  "keywords": [
    "electron",
    "web-audio-api",
    "pitch",
    "audio"
  ],
